set terminal pngcairo dashed
#set key outside
set xlabel "seconds"
set ylabel "kbps" 
set yrange [0 to 8000]
set arrow from 62.081515, graph(0,0) to 62.081515, graph(1,1) nohead dashtype 2
set arrow from 142.100128, graph(0,0) to 142.100128, graph(1,1) nohead dashtype 2
set output "figures/youtube_combined.png"
set title "YouTube throughput and video playback rate vs time (6Mbps bottleneck)"
